include $(MAKE_INCLUDE_GEN)

.PHONY: all clean

#*****************************************************************************#
#                               VARIABLES	                              #
#*****************************************************************************#

#*****************************************************************************#
#                        RULES TO GENERATE TARGETS                            #
#*****************************************************************************#
all:
ifeq ($(CONFIG_UCGUI_EN),y)
	@make -C ucgui all
endif
ifeq ($(CONFIG_LITTLEVGL_EN),y)
	@make -C LittlevGL all
endif

#*****************************************************************************#
#              CLEAN GENERATED FILES                                          #
#*****************************************************************************#
clean:
	@make -C ucgui clean
	@make -C LittlevGL clean

-include $(DEPS)
