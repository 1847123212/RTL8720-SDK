include $(MAKE_INCLUDE_GEN)

.PHONY: all clean

#*****************************************************************************#
#                               VARIABLES	                              #
#*****************************************************************************#

#*****************************************************************************#
#                        RULES TO GENERATE TARGETS                            #
#*****************************************************************************#
all: mk_copy	
	@make -C src all

mk_copy:
	$(COPY) $(ROOTDIR)/../src/Makefile ./src/

#*****************************************************************************#
#              CLEAN GENERATED FILES                                          #
#*****************************************************************************#
clean: mk_copy
	@make -C src clean

-include $(DEPS)
